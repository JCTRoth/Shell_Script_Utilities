# Safe package blacklist for adb_bloatware_remover.sh
# These packages should NEVER be uninstalled or disabled as they are critical for Android functionality
# One package name per line, comments with #

#
# Stock Android
#
android                         # Core Android package
com.android.phone               # Phone/dialer functionality
com.android.providers.downloads # Downloads provider
com.android.providers.media     # Media provider
com.android.providers.settings  # System settings provider
com.android.systemui            # System UI (status bar, navigation)
com.android.vending             # Google Play Store
com.google.android.gsf          # Google Services Framework
com.google.android.gms          # Google Play Services

# Additional critical system packages
com.android.bluetooth           # Bluetooth service
com.android.contacts            # Contacts provider
com.android.dialer              # Phone dialer app
com.android.inputmethod.latin   # Default Latin IME (keyboard)
com.android.location.fused      # Fused location provider
com.android.mms                 # Messaging/SMS app
com.android.providers.contacts  # Contacts database provider
com.android.providers.telephony # Telephony provider
com.android.server.telecom      # Telecom server
com.android.settings            # System Settings app
com.android.wifi                # WiFi service

